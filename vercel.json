{
    "functions": {
      "api/**.[jt]s": {
        "source": "api_vercel/api"
      }
    },
    "buildCommand": "expo export -p web",
    "framework": null,
    "outputDirectory": "dist",
    "cleanUrls": true,
    "routes": [
      { "src": "/api/(.*)", "dest": "api/$1.ts" },   
      { "handle": "filesystem" },                    
      { "src": "/(.*)", "dest": "/index.html" }
    ]
}