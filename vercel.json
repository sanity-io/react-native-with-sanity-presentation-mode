{
    "functions": {
      "api/**.[jt]s": {
        "includeFiles": "api/**.[jt]s"
      }
    },
    "framework": null,
    "outputDirectory": "dist",
    "cleanUrls": true,
    "rewrites": [
      { "source": "/api/(.*)", "destination": "api/routes/$1.ts" },
      { "source": "/(.*)", "destination": "expo/dist/index.html" }
    ]
}